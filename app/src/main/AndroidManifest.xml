    <application
        android:allowBackup="true"
        android:icon="@mipmap/gro_icon"
        android:label="gronline"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Grocery_new"
